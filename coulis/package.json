{
	"name": "coulis",
	"version": "0.19.0",
	"description": "Yet another atomic CSS-in-JS library",
	"keywords": [
		"css",
		"js",
		"css-in-js",
		"atomic"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:adbayb/coulis.git",
		"directory": "packages/coulis"
	},
	"license": "MIT",
	"author": {
		"name": "Ayoub Adib",
		"email": "adbayb@gmail.com",
		"url": "https://twitter.com/adbayb"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/web.d.ts",
			"import": "./dist/web.mjs",
			"require": "./dist/web.cjs",
			"default": "./dist/web.mjs"
		},
		"./web": {
			"source": "./src/adapters/web/index.ts",
			"types": "./dist/web.d.ts",
			"import": "./dist/web.mjs",
			"require": "./dist/web.cjs",
			"default": "./dist/web.mjs"
		},
		"./react-native": {
			"source": "./src/adapters/react-native/index.ts",
			"types": "./dist/react-native.d.ts",
			"import": "./dist/react-native.mjs",
			"require": "./dist/react-native.cjs",
			"default": "./dist/react-native.mjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"prebuild": "pnpm run clean",
		"build": "quickbundle build",
		"clean": "rm -rf dist",
		"prepublishOnly": "pnpm run build",
		"start": "pnpm watch",
		"test": "vitest --typecheck",
		"watch": "quickbundle watch"
	},
	"dependencies": {
		"csstype": "^3.2.3"
	},
	"devDependencies": {
		"quickbundle": "2.16.0",
		"react-native": "0.83.1",
		"vitest": "3.2.4"
	},
	"peerDependencies": {
		"react-native": "^0.80.0 || ^0.81.0 || ^0.82.0 || ^0.83.0"
	},
	"peerDependenciesMeta": {
		"react-native": {
			"optional": true
		}
	}
}
