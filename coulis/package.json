{
	"name": "coulis",
	"version": "0.15.0",
	"description": "Yet another atomic CSS-in-JS library",
	"keywords": [
		"css",
		"js",
		"css-in-js",
		"atomic"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:adbayb/coulis.git",
		"directory": "packages/coulis"
	},
	"license": "MIT",
	"author": {
		"name": "Ayoub Adib",
		"email": "adbayb@gmail.com",
		"url": "https://twitter.com/adbayb"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"source": "./src/adapters/web/index.ts",
			"types": "./dist/web.d.ts",
			"require": "./dist/web.cjs",
			"import": "./dist/web.mjs"
		},
		"./web": {
			"source": "./src/adapters/web/index.ts",
			"types": "./dist/web.d.ts",
			"require": "./dist/web.cjs",
			"import": "./dist/web.mjs"
		},
		"./react-native": {
			"source": "./src/adapters/react-native/index.ts",
			"types": "./dist/react-native.d.ts",
			"require": "./dist/react-native.cjs",
			"import": "./dist/react-native.mjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"prebuild": "pnpm run clean",
		"build": "quickbundle build",
		"clean": "rm -rf dist",
		"prepublishOnly": "pnpm run build",
		"start": "pnpm watch",
		"test": "vitest --typecheck",
		"watch": "quickbundle watch"
	},
	"dependencies": {
		"csstype": "^3.1.3"
	},
	"devDependencies": {
		"@types/node": "22.15.34",
		"quickbundle": "2.13.0",
		"vitest": "3.2.4"
	}
}
